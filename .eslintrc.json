{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      1,
      "double",
      "avoid-escape"
    ],
    "indent": [
      0,
      4
    ],
    "no-console": [
      0
    ],
    "no-underscore-dangle": [
      0
    ],
    "class-methods-use-this": [
      0
    ],
    "import/prefer-default-export": [
      0
    ],
    "max-len": [
      0,
      80
    ],
    "no-case-declarations": [
      0
    ],
    "operator-linebreak": [
      0
    ],
    "import/no-extraneous-dependencies": [
      0
    ],
    "import/extensions": [
      0
    ],
    "no-await-in-loop": [
      0
    ],
    "no-restricted-syntax": [
      0
    ],
    "no-param-reassign": [
      0
    ],
    "no-promise-executor-return": [
      0
    ]
  },
  "ignorePatterns": [
    "node_modules/*"
  ]
}
